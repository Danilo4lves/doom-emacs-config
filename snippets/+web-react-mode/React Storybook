// Dependencies
import React from 'react';
import { storiesOf } from '@storybook/react';

// Component
import ${1:componentName} from './';

function defaultStory() {
  return (
    <$1 />
  );
}

storiesOf('$1', module)
	.add('Default', defaultStory);